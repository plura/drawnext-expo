{

	"jobs": [
		{
			"source": "./backend/api",
			"output": "./.export/chatgpt-backend-api.txt"
		},
		{
			"source": "./backend/lib",
			"output": "./.export/chatgpt-backend-lib.txt"
		},
		{
			"source": "./backend/scripts",
			"output": "./.export/chatgpt-backend-scripts.txt"
		},
		{
			"source": "./frontend/src",
			"output": "./.export/chatgpt-frontend.txt",
