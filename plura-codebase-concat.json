				"components/ui",
				"features/admin",
				"features/submit"
			]
		},
		{
			"source": "./frontend/src/features/admin",
			"output": "./.export/chatgpt-frontend-admin.txt"
		},
		{
			"source": "./frontend/src/features/submit",
			"output": "./.export/chatgpt-frontend-submit.txt"
		}
	]
